{
  "name": "numenta-web-site-com",
  "version": "0.2.200",
  "description": "Numenta.com company website content, source code, and static generator tooling",
  "license": "MIT",
  "main": false,
  "private": true,
  "author": {
    "name": "Numenta",
    "email": "info@numenta.com",
    "url": "http://numenta.com"
  },
  "bugs": {
    "url": "https://github.com/numenta/numenta-web/issues",
    "email": "info@numenta.com"
  },
  "dependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "classnames": "2.2.5",
    "extract-text-webpack-plugin": "github:michelebertoli/extract-text-webpack-plugin#614c3bb1ed72587218aa644df12bbf514fb77a06",
    "favicons-webpack-plugin": "0.0.7",
    "front-matter": "2.1.0",
    "gatsby": "0.11.7",
    "history": "2.1.1",
    "html2plaintext": "1.0.1",
    "js-yaml": "3.6.1",
    "moment": "2.15.2",
    "ncp": "2.0.0",
    "numenta-web-shared-components": "^0.0.0",
    "numenta-web-shared-utils": "^0.0.0",
    "platform": "1.3.2",
    "react": "15.3.2",
    "react-addons-pure-render-mixin": "15.3.2",
    "react-dom": "15.3.2",
    "react-g-analytics": "0.2.6",
    "react-helmet": "3.2.0",
    "react-icons": "2.2.1",
    "react-modal": "1.5.2",
    "react-router": "2.8.1",
    "react-tap-event-plugin": "1.0.0",
    "sitemap": "1.8.2",
    "tachyons": "github:numenta/tachyons#d8e73dec47cb1c868bde3e4e4d5fd60fa5483fe2",
    "tachyons-base": "1.2.6"
  },
  "devDependencies": {
    "babel-eslint": "7.1.0",
    "babel-jest": "16.0.0",
    "babel-polyfill": "6.16.0",
    "broken-link-checker": "0.7.3",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jasmine": "2.1.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0",
    "gh-pages": "0.11.0",
    "html5-lint": "0.2.5",
    "htmllint-cli": "0.0.5",
    "identity-obj-proxy": "3.0.0",
    "jasmine-expect": "3.0.1",
    "jest": "16.0.2",
    "open-url": "2.0.2",
    "react-addons-test-utils": "15.3.2",
    "react-stub-context": "0.7.0",
    "react-test-renderer": "15.3.2",
    "shx": "0.1.4",
    "stylelint": "7.5.0",
    "stylelint-config-standard": "14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numenta/numenta-web"
  },
  "scripts": {
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "gatsby build --prefix-links",
    "clean": "npm run clean:build",
    "clean:build": "shx rm -rf public/",
    "clean:npm": "shx rm -rf node_modules/ && npm prune && npm cache clear && npm cache clean",
    "clean:test": "shx rm -rf coverage/",
    "deploy:gh-pages": "gh-pages --dotfiles --dist public/",
    "predev": "npm run lint",
    "dev": "gatsby develop",
    "lint": "npm run lint:css && npm run lint:html && npm run lint:js",
    "lint:css": "stylelint --config=../../.stylelintrc \"**/*.css\"",
    "lint:html": "echo",
    "lint:js": "eslint --config=../../.eslintrc.json --ext=js --ext=jsx .",
    "preserve": "npm run build",
    "serve": "gatsby serve-build",
    "pretest": "npm run clean:test",
    "test": "npm run test:unit",
    "test:links": "blc --exclude \"amazon.com/On-Intel\" --exclude \"linkedin.com/company/numenta\" --get --ordered --recursive",
    "test:links:local": "npm run test:links -- http://0.0.0.0:8000",
    "test:links:prod": "npm run test:links -- http://numenta.com",
    "test:links:stage": "npm run test:links -- http://staging.numenta.com",
    "test:unit": "jest --config .jestrc.json \"unit.test\"",
    "test:unit:cover": "npm run test:unit -- --coverage && open-url file://$(shx pwd)/coverage/lcov-report/index.html",
    "test:unit:update": "npm run test:unit -- -u",
    "test:unit:watch": "npm run test:unit -- --watch",
    "validate": "npm run validate:html",
    "validate:html": "echo"
  }
}
